#include <stdio.h>

int main()
{
    int num=0;
   
    
    while (num <1 || num >10){
            printf("\nDigite o Multiplicador: ");
            scanf("%d", &num);
            printf("\nO numero tem que ser entre 1 e 10");
          }
    for(int i = 1; i<11; i++){
        int tabuada = i * num;
        printf("%d x %d = %d\n", num, i, tabuada);    
    }
       
}
